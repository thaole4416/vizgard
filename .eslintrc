{
    "parser": "@typescript-eslint/parser",
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "accessor-pairs": [1, { "setWithoutGet": true, "getWithoutSet": true }],
        "array-bracket-newline": 0,
        "array-bracket-spacing": 1,
        "array-callback-return": 1,
        "array-element-newline": 0,
        "arrow-body-style": [1, "as-needed"],
        "arrow-parens": [0, "as-needed"],
        "arrow-spacing": 1,
        "block-scoped-var": 2,
        "block-spacing": 1,
        "brace-style": 0,
        "callback-return": 0,
        "camelcase": 0,
        "capitalized-comments": 0,
        "class-methods-use-this": 0,
        "comma-dangle": [1, "always-multiline"],
        "comma-spacing": 1,
        "comma-style": 1,
        "complexity": [1, { "max": 30 }],
        "computed-property-spacing": 1,
        "consistent-return": 0,
        "consistent-this": 1,
        "constructor-super": 2,
        "curly": [1, "multi-line", "consistent"],
        "default-case": 1,
        "dot-location": [1, "property"],
        "dot-notation": 1,
        "eol-last": 1,
        "eqeqeq": [1, "smart"],
        "for-direction": 2,
        "func-call-spacing": 1,
        "func-name-matching": 1,
        "func-names": 0,
        "func-style": 0,
        "function-paren-newline": 0,
        "generator-star-spacing": [1, "after"],
        "getter-return": 2,
        "global-require": 1,
        "guard-for-in": 1,
        "handle-callback-err": 1,
        "id-blacklist": 0,
        "id-length": 0,
        "id-match": 0,
        "implicit-arrow-linebreak": 0,
        "indent": 0,
        "init-declarations": 0,
        "jsx-quotes": 1,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "line-comment-position": 0,
        "linebreak-style": 1,
        "lines-around-comment": 0,
        "lines-between-class-members": 1,
        "max-classes-per-file": [1, 2],
        "max-depth": [1, 5],
        "max-len": [1, { "code": 200, "tabWidth": 2 }],
        "max-lines-per-function": 0,
        "max-lines": [1, 1000],
        "max-nested-callbacks": [1, 10],
        "max-params": [1, 10],
        "max-statements-per-line": [1, { "max": 3 }],
        "max-statements": [1, 50],
        "multiline-comment-style": 0,
        "multiline-ternary": 0,
        "new-cap": 0,
        "new-parens": 1,
        "newline-per-chained-call": 0,
        "no-alert": 1,
        "no-array-constructor": 1,
        "no-async-promise-executor": 2,
        "no-await-in-loop": 0,
        "no-bitwise": 0,
        "no-buffer-constructor": 1,
        "no-caller": 2,
        "no-case-declarations": 1,
        "no-class-assign": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": [2, "always"],
        "no-confusing-arrow": 0,
        "no-console": 0,
        "no-const-assign": 2,
        "no-constant-condition": 1,
        "no-continue": 0,
        "no-control-regex": 1,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 1,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-duplicate-imports": 2,
        "no-else-return": 1,
        "no-empty-character-class": 1,
        "no-empty-function": [1, { "allow": ["arrowFunctions"] }],
        "no-empty-pattern": 1,
        "no-empty": 2,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-ex-assign": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 2,
        "no-extra-label": 1,
        "no-extra-parens": [1, "all", { "nestedBinaryExpressions": false, "ignoreJSX": "multi-line" }],
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 2,
        "no-global-assign": 1,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 1,
        "no-implied-eval": 1,
        "no-inline-comments": 0,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 1,
        "no-iterator": 2,
        "no-label-var": 2,
        "no-labels": 1,
        "no-lone-blocks": 2,
        "no-lonely-if": 1,
        "no-loop-func": 1,
        "no-magic-numbers": 0,
        "no-misleading-character-class": 1,
        "no-mixed-operators": 0,
        "no-mixed-requires": 2,
        "no-mixed-spaces-and-tabs": [1, "smart-tabs"],
        "no-multi-assign": 0,
        "no-multi-spaces": 1,
        "no-multi-str": 2,
        "no-multiple-empty-lines": [1, { "max": 1 }],
        "no-negated-condition": 0,
        "no-nested-ternary": 1,
        "no-new-func": 2,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-symbol": 2,
        "no-new-wrappers": 2,
        "no-new": 1,
        "no-obj-calls": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": 1,
        "no-path-concat": 1,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 1,
        "no-proto": 2,
        "no-prototype-builtins": 1,
        "no-redeclare": 2,
        "no-regex-spaces": 1,
        "no-restricted-globals": 0,
        "no-restricted-imports": 0,
        "no-restricted-modules": 0,
        "no-restricted-properties": 0,
        "no-restricted-syntax": 0,
        "no-return-assign": 2,
        "no-return-await": 1,
        "no-script-url": 2,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 1,
        "no-shadow-restricted-names": 2,
        "no-shadow": [1, { "allow": ["err", "next", "res", "callback"] }],
        "no-sparse-arrays": 1,
        "no-sync": 1,
        "no-tabs": 0,
        "no-template-curly-in-string": 1,
        "no-ternary": 0,
        "no-this-before-super": 2,
        "no-throw-literal": 1,
        "no-trailing-spaces": 1,
        "no-undef-init": 1,
        "no-undef": 2,
        "no-undefined": 0,
        "no-underscore-dangle": 1,
        "no-unexpected-multiline": 1,
        "no-unmodified-loop-condition": 1,
        "no-unneeded-ternary": 1,
        "no-unreachable": 2,
        "no-unsafe-finally": 1,
        "no-unsafe-negation": 2,
        "no-unused-expressions": 2,
        "no-unused-labels": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-useless-call": 2,
        "no-useless-computed-key": 1,
        "no-useless-concat": 2,
        "no-useless-constructor": 2,
        "no-useless-escape": 2,
        "no-useless-rename": 2,
        "no-useless-return": 1,
        "no-var": 1,
        "no-void": 1,
        "no-warning-comments": 0,
        "no-whitespace-before-property": 1,
        "no-with": 1,
        "nonblock-statement-body-position": 1,
        "object-curly-newline": 0,
        "object-curly-spacing": [1, "always"],
        "object-property-newline": 0,
        "object-shorthand": 1,
        "one-var-declaration-per-line": 0,
        "one-var": 0,
        "operator-assignment": 1,
        "operator-linebreak": 0,
        "padded-blocks": 0,
        "padding-line-between-statements": 0,
        "prefer-arrow-callback": 1,
        "prefer-const": 1,
        "prefer-destructuring": 1,
        "prefer-numeric-literals": 1,
        "prefer-object-spread": 1,
        "prefer-promise-reject-errors": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "prefer-template": 1,
        "quote-props": 0,
        "quotes": [1, "single", "avoid-escape"],
        "radix": 0,
        "react-hooks/exhaustive-deps": 1,
        "react-hooks/rules-of-hooks": 2,
        "react/boolean-prop-naming": 0,
        "react/button-has-type": 1,
        "react/default-props-match-prop-types": 1,
        "react/destructuring-assignment": 0,
        "react/display-name": 0,
        "react/forbid-component-props": 0,
        "react/forbid-dom-props": 0,
        "react/forbid-elements": 0,
        "react/forbid-foreign-prop-types": 1,
        "react/forbid-prop-types": 0,
        "react/jsx-boolean-value": 1,
        "react/jsx-child-element-spacing": 0,
        "react/jsx-closing-bracket-location": 1,
        "react/jsx-closing-tag-location": 1,
        "react/jsx-curly-brace-presence": 1,
        "react/jsx-curly-spacing": 1,
        "react/jsx-equals-spacing": 1,
        "react/jsx-filename-extension": 0,
        "react/jsx-first-prop-new-line": 0,
        "react/jsx-handler-names": 0,
        "react/jsx-indent-props": [1, 2],
        "react/jsx-indent": [1, 2],
        "react/jsx-key": 1,
        "react/jsx-max-depth": [1, { "max": 10 }],
        "react/jsx-max-props-per-line": [1, { "maximum": 5 }],
        "react/jsx-no-bind": 0,
        "react/jsx-no-comment-textnodes": 1,
        "react/jsx-no-duplicate-props": 2,
        "react/jsx-no-literals": 0,
        "react/jsx-no-target-blank": 1,
        "react/jsx-no-undef": 2,
        "react/jsx-one-expression-per-line": 0,
        "react/jsx-pascal-case": 1,
        "react/jsx-props-no-multi-spaces": 1,
        "react/jsx-sort-default-props": 0,
        "react/jsx-sort-props": 0,
        "react/jsx-space-before-closing": 0,
        "react/jsx-tag-spacing": [            1,            {                "closingSlash": "never",                "beforeSelfClosing": "never",                "afterOpening": "never"            }        ],
        "react/jsx-uses-react": "off",
        "react/jsx-uses-vars": 1,
        "react/jsx-wrap-multilines": 0,
        "react/no-access-state-in-setstate": 1,
        "react/no-array-index-key": 1,
        "react/no-children-prop": 1,
        "react/no-danger-with-children": 1,
        "react/no-danger": 1,
        "react/no-deprecated": 1,
        "react/no-did-mount-set-state": 1,
        "react/no-did-update-set-state": 1,
        "react/no-direct-mutation-state": 1,
        "react/no-find-dom-node": 1,
        "react/no-is-mounted": 2,
        "react/no-multi-comp": 0,
        "react/no-redundant-should-component-update": 1,
        "react/no-render-return-value": 1,
        "react/no-set-state": 0,
        "react/no-string-refs": 1,
        "react/no-this-in-sfc": 1,
        "react/no-typos": 1,
        "react/no-unescaped-entities": 2,
        "react/no-unknown-property": 2,
        "react/no-unsafe": 1,
        "react/no-unused-prop-types": 1,
        "react/no-unused-state": 1,
        "react/no-will-update-set-state": 1,
        "react/prefer-es6-class": 1,
        "react/prefer-stateless-function": 1,
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "react/require-default-props": 1,
        "react/require-optimization": 0,
        "react/require-render-return": 2,
        "react/self-closing-comp": 1,
        "react/sort-comp": 1,
        "react/sort-prop-types": 0,
        "react/style-prop-object": 1,
        "react/void-dom-elements-no-children": 1,
        "require-atomic-updates": 1,
        "require-await": 1,
        "require-jsdoc": 0,
        "require-unicode-regexp": 0,
        "require-yield": 2,
        "rest-spread-spacing": 1,
        "semi-spacing": 1,
        "semi-style": 1,
        "semi": 1,
        "sort-imports": 0,
        "sort-keys": 0,
        "sort-vars": 0,
        "space-before-blocks": 1,
        "space-before-function-paren": 0,
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "strict": 0,
        "switch-colon-spacing": 1,
        "symbol-description": 1,
        "template-curly-spacing": 1,
        "template-tag-spacing": 1,
        "unicode-bom": 1,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "vars-on-top": 0,
        "wrap-iife": 1,
        "wrap-regex": 0,
        "yield-star-spacing": 1,
        "yoda": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
